lds:Mitte rdfs:label "Stadtbezirk Mitte"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Zentrum, ldot:Zentrum-Nordwest,
    ldot:Zentrum-Sued, ldot:Zentrum-Suedost, ldot:Zentrum-West,
    ldot:Zentrum-Ost, ldot:Zentrum-Nord .

lds:NordOst rdfs:label "Stadtbezirk Nord-Ost"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Schoenefeld-Abtnaundorf,
    ldot:Schoenefeld-Ost, ldot:Mockau-Nord, ldot:Mockau-Sued,
    ldot:Plaussig-Portitz, ldot:Thekla .

lds:Ost rdfs:label "Stadtbezirk Ost"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Althen-Kleinpoesna, ldot:Baalsdorf,
    ldot:Anger-Crottendorf, ldot:Engelsdorf, ldot:Heiterblick,
    ldot:Neustadt-Neuschoenefeld, ldot:Sellerhausen-Stuenz, ldot:Moelkau,
    ldot:Paunsdorf, ldot:Volkmarsdorf .

lds:SuedOst rdfs:label "Stadtbezirk Süd-Ost"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Reudnitz-Thonberg, ldot:Holzhausen,
    ldot:Liebertwolkwitz, ldot:Meusdorf, ldot:Probstheida, ldot:Stoetteritz .

lds:Sued  rdfs:label "Stadtbezirk Süd"; a ld:Stadtbezirk ;
   ld:containsOrtsteil ldot:Connewitz, ldot:Doelitz-Doesen,
    ldot:Loessnig, ldot:Marienbrunn, ldot:Suedvorstadt.

lds:SuedWest rdfs:label "Stadtbezirk Süd-West"; a ld:Stadtbezirk ;
    rdfs:label "Stadtbezirk Mitte"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Grosszschocher,
    ldot:Hartmannsdorf-Knautnaundorf, ldot:Kleinzschocher,
    ldot:Knautkleeberg-Knauthain, ldot:Plagwitz, ldot:Schleussig .
 
lds:West  rdfs:label "Stadtbezirk West"; a ld:Stadtbezirk ;
   ld:containsOrtsteil ldot:Gruenau-Mitte, ldot:Gruenau-Nord,
    ldot:Gruenau-Ost, ldot:Gruenau-Siedlung, ldot:Lausen-Gruenau,
    ldot:Miltitz, ldot:Schoenau .
 
lds:AltWest rdfs:label "Stadtbezirk Alt-West"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Altlindenau, ldot:Boehlitz-Ehrenberg,
    ldot:Burghausen-Rueckmarsdorf, ldot:Leutzsch, ldot:Lindenau,
    ldot:Neulindenau .
 
lds:NordWest rdfs:label "Stadtbezirk Nord-West"; a ld:Stadtbezirk ;
    ld:containsOrtsteil ldot:Lindenthal, ldot:Luetzschena-Stahmeln,
    ldot:Moeckern, ldot:Wahren .


lds:Nord  rdfs:label "Stadtbezirk Nord"; a ld:Stadtbezirk ;
   ld:containsOrtsteil ldot:Eutritzsch, ldot:Gohlis-Mitte,
   ldot:Gohlis-Nord, ldot:Gohlis-Sued, ldot:Wiederitzsch, ldot:Seehausen .

