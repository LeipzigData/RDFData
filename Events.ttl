                                                   ical:sentBy <http://leipzig-data.de/Data/Agent/NEU.Events> ;
                                                   ical:contact "Herr Dr. Jürgen Wummel, Frau Ute Liebelt [ute.liebelt@leipzig.de]" ;
                                                   ld:hasMD5Sum "/WL4TLpnexzJtSFnuszCLQ" .

