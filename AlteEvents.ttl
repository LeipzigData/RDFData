                                                   ical:organizer ldo:KreativesLeipzig ;
                                                   ical:sentBy <http://leipzig-data.de/Data/Agent/APILeipzig.Events> ;
                                                   ical:summary "WebMontag zur Dachmarke \"L\"" ;
                                                   ical:url <https://www.facebook.com/events/426894430839501/> ;
                                                   ld:hasAPIRef <http://leipzig-data.de/Data/APIId/Event.303>, <http://leipzig-data.de/Data/APIId/Event.304>, <http://leipzig-data.de/Data/APIId/Event.305> ;
