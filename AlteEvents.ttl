                                                   ical:organizer ldo:NetzwerkEnergieUmwelt ;
                                                   ical:sentBy <http://leipzig-data.de/Data/Agent/NEU.Events> ;
                                                   ical:summary "Energiemetropole-Leipzig: Ringvorlesung Universität Leipzig: Totgesagte leben länger – Herausforderung Photovoltaik" ;
                                                   ical:url <http://www.energieverein-leipzig.de/kraftakt/> ;
                                                   ld:hasMD5Sum "RMI3U5prVzQHExoWgR2mJg" ;
                                                   ld:hasTag ldtag:Energie ;
