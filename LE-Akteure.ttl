<http://leipziger-ecken.de/Data/Ort/O64> a le:Ort  ;
   le:hasAID "64" ;
   ld:hasSupplier <http://leipziger-ecken.de/Data/Akteur/A64> ;
   foaf:name "ArbeitsladenPLUS Leipziger Osten" ;
