   ical:dtstart "2018-08-18T12:00:00.000+02:00" ;
   ical:dtend "2018-08-19T15:00:00.000+02:00" ;
   nl:hasTargetGroup "interessierte Erwachsene" ;
   nl:hasCosts "80€ (Ermäßigungen bitte anfragen!)" ;
   nl:hasRequirements """12 Uhr Treff in Leipzig zur gemeinsamen Fahrt zu einem Waldgebiet bei  Leipzig
Bei Interesse bitte Anmeldung über:
info@natur-wildnisschule-leipzig.de
Tel.: 0171.1026993""" ;
