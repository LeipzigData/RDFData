   nl:hasFullAddress "Weißenfelser Straße 65H, 04229 Leipzig" ;
   ld:proposedAddress <http://leipzig-data.de/Data/04229.Leipzig.WeissenfelserStrasse.65H> ;
   nl:hasInfoURL <https://www.degrowth.de/de/dib/degrowth-in-bewegungen/#Buchvorstellung> ;
   nl:hasImageURL <http://daten.nachhaltiges-leipzig.de/system/event/image/00000366/DIB.jpg> ;
   ical:dtstart "2017-03-25T19:30:00.000+01:00" ;
   ical:dtend "2017-03-25T21:00:00.000+01:00" ;
