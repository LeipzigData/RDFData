   ld:proposedAddress <http://leipzig-data.de/Data/04177.Leipzig.Georg-Schwarz-Strasse.7> ;
   nl:hasImageURL <http://daten.nachhaltiges-leipzig.de/system/event/image/00000499/Work-Hard-Play-Hard.jpg> ;
   ical:dtstart "2017-10-26T19:00:00.000+02:00" ;
   ical:dtend "2017-10-26T23:00:00.000+02:00" ;
